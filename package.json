{
  "name": "react-etalpreliob",
  "version": "0.0.0",
  "dependencies": {
    "@hot-loader/react-dom": "^16.11.0",
    "ky": "^0.16.1",
    "ky-universal": "^0.3.0",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "react-helmet": "^5.2.1",
    "react-hot-loader": "^4.12.18",
    "react-router-dom": "^5.1.2"
  },
  "devDependencies": {
    "@commitlint/cli": "^8.3.4",
    "@commitlint/config-conventional": "^8.3.4",
    "@storybook/addons": "^5.3.0-rc.11",
    "@storybook/preset-create-react-app": "^1.5.0",
    "@storybook/react": "^5.3.0-rc.11",
    "@testing-library/jest-dom": "^4.2.4",
    "@testing-library/react": "^9.4.0",
    "@testing-library/user-event": "^8.0.3",
    "@types/jest": "^24.0.25",
    "@types/node": "^13.1.4",
    "@types/react": "^16.9.17",
    "@types/react-dom": "^16.9.4",
    "@types/react-helmet": "^5.0.15",
    "@types/react-router": "^5.1.3",
    "@types/react-router-dom": "^5.1.3",
    "cross-env": "^6.0.3",
    "customize-cra": "^0.9.1",
    "cz-conventional-changelog": "^3.0.2",
    "full-icu": "^1.3.0",
    "husky": "^3.1.0",
    "identity-obj-proxy": "^3.0.0",
    "lint-staged": "^9.5.0",
    "node-sass": "^4.13.0",
    "prettier": "^1.19.1",
    "react-app-rewired": "^2.1.5",
    "react-scripts": "3.3.0",
    "react-snap": "^1.23.0",
    "stylelint": "^12.0.1",
    "stylelint-a11y": "^1.2.2",
    "stylelint-config-recommended": "^3.0.0",
    "stylelint-declaration-block-no-ignored-properties": "^2.2.0",
    "stylelint-no-indistinguishable-colors": "^1.3.0",
    "stylelint-rscss": "^0.4.0",
    "stylelint-scss": "^3.13.0",
    "typescript": "^3.7.4",
    "webpack-bundle-analyzer": "^3.6.0"
  },
  "scripts": {
    "start": "react-app-rewired start",
    "build": "react-app-rewired build",
    "postbuild": "react-snap",
    "test": "cross-env NODE_ICU_DATA=node_modules/full-icu react-app-rewired test --coverage",
    "lint": "npm run lint:js && npm run lint:css",
    "lint:js": "eslint --fix ./src/**/*.{js,jsx,ts,tsx}",
    "lint:css": "stylelint --fix ./src/**/*.{css,scss}",
    "prettify": "prettier --write ./src/**/*.{css,scss,js,jsx,ts,tsx}",
    "setup": "node setup.js",
    "commit": "npx git-cz",
    "storybook": "start-storybook -p 9009 -s public",
    "build-storybook": "build-storybook -s public"
  },
  "jest": {
    "moduleNameMapper": {
      "#(.*)$": "<rootDir>/src/$1",
      "^.+\\.scss$": "identity-obj-proxy"
    },
    "coverageDirectory": "<rootDir>/.internals/coverage",
    "setupFilesAfterEnv": [
      "<rootDir>/.internals/jest.js"
    ]
  },
  "babel": {
    "plugins": [
      "react-hot-loader/babel"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "stylelint": {
    "extends": [
      "stylelint-config-recommended",
      "stylelint-rscss/config",
      "stylelint-a11y/recommended"
    ],
    "plugins": [
      "stylelint-scss",
      "stylelint-no-indistinguishable-colors",
      "stylelint-declaration-block-no-ignored-properties"
    ],
    "rules": {
      "plugin/stylelint-no-indistinguishable-colors": true,
      "plugin/declaration-block-no-ignored-properties": true
    }
  },
  "lint-staged": {
    "*.{css,scss,js,jsx,ts,tsx}": [
      "npm run prettify"
    ],
    "*.{css,scss}": [
      "npm run lint:css"
    ],
    "*.{js,jsx,ts,tsx}": [
      "npm run lint:js"
    ]
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "config-overrides-path": ".internals/override.js"
}
